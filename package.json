{
  "name": "@seven332/libheif-wasm",
  "version": "0.1.0",
  "description": "A WebAssembly implementation of heif decoder",
  "homepage": "https://github.com/seven332/libheif-wasm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seven332/libheif-wasm.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-wasm-debug": "./tools/build_wasm.sh debug",
    "build-wasm-release": "./tools/build_wasm.sh release",
    "build": "tsc && cp src/libheif.js src/libheif.wasm dist",
    "test": "jest",
    "clean": "rm -rf build dist",
    "clean-build": "npm run clean && npm run build-wasm-release && npm run build",
    "prepublishOnly": "npm run clean-build && npm test",
    "build-demo": "npm run build && cp dist/index.js dist/libheif.js dist/libheif.wasm demo"
  },
  "keywords": [
    "heif",
    "heic",
    "avif",
    "wasm"
  ],
  "author": "seven332",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
